{
    "version": 2,
    "public": true,
    "cleanUrls": true,
    "framework": "nextjs",
    "installCommand": "npm install",
    "buildCommand": "npm run build --workspace=@antigravity/portal",
    "outputDirectory": "apps/portal/.next",
    "rewrites": [
        {
            "source": "/sub-apps-api/:path*",
            "destination": "/api/:path*"
        }
    ]
}